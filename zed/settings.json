{
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "Iosevka",
  "base_keymap": "JetBrains",
  "relative_line_numbers": true,
  "autosave": "on_focus_change",
  "format_on_save": "off",
  "remove_trailing_whitespace_on_save": false,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-thinking-latest"
    },
    "version": "2"
  },
  "jupyter": {
    "enabled": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
